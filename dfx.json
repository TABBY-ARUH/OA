{
  "canisters": {
    "openArb-ui-backend": {
      "type": "rust",
      "package": "openArb-ui-backend",
      "candid": "src/openArb-ui-backend/openArb-ui-backend.did"
    },
    "openArb-ui-frontend": {
      "type": "assets",
      "dependencies": [
        "openArb-ui-backend"
      ],
      "source": [
        "src/openArb-ui-frontend/dist"
      ],
      "build": [
        "npm install --prefix src/openArb-ui-frontend",
        "npm run build --prefix src/openArb-ui-frontend"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
